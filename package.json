  "scripts": {
    "ng": "ng",
    "local": "ionic serve --port 8080",
    "local-secure": "ionic serve --port 8080 --ssl true --disable-host-check",
    "local-public": "ionic serve --external 0.0.0.0 --port 8080 --disable-host-check",
    "local:vdi": "ng serve --port 8080 --poll 2000",
    "stage": "ionic serve --configuration stage --port 4000",
    "build": "ng build",
    "build:aot": "ng build --aot=true",
    "build:dev": "ng build --outputPath dist/dev --stats-json",
    "build:uat": "ng build --configuration staging --prod --outputPath dist/uat",
    "build:prod": "ng build --configuration production --prod --outputPath dist/prod",
    "build:cd-dev": "ng build --aot=true --outputPath dist/ --stats-json && rm -f dist/*.tar && tar -czvf apac-labelex-${npm_package_version}.tar dist/* && mv apac-labelex-${npm_package_version}.tar dist && ls -l dist/",
    "build:cd-uat": "ng build --configuration staging --prod --outputPath dist/ && rm -f dist/*.tar && tar -czvf apac-labelex-${npm_package_version}.tar dist/* && mv apac-labelex-${npm_package_version}.tar dist && ls -l dist/",
    "build:cd-prod": "ng build --configuration production --prod --outputPath dist/ && rm -f dist/*.tar && tar -czvf apac-labelex-${npm_package_version}.tar dist/* && mv apac-labelex-${npm_package_version}.tar dist && ls -l dist/",
    "test": "ng test --code-coverage",
    "test-report": "ng test --reporters=html",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-dev": "ng e2e --configuration=development",
    "e2e-local": "ng e2e --configuration=local --port 8080 --no-webdriver-update",
    "e2e-release": "ng e2e --configuration=release",
    "webdriver-manager": "./node_modules/protractor/bin/webdriver-manager update --versions.chrome=96.0.4664.45",
    "allure-report": "allure generate allure-results --clean -o allure-report",
    "sonar": "sonar-scanner",
    "postinstall": "ngcc",
    "dev:ssr": "ng run app:serve-ssr",
    "serve:ssr": "node dist/app/server/main.js",
    "build:ssr": "ng build --configuration production --prod && ng run app:server:production",
    "serve:nodev12:ssr": "z:\\node-v12.14.1-win-x64\\node dist/app/server/main.js",
    "build:nodev12:ssr": "z:\\node-v12.14.1-win-x64\\ng.cmd build --configuration production --prod && z:\\node-v12.14.1-win-x64\\ng.cmd run app:server:production",
    "build:uat:ssr": "ng build --configuration staging --prod --outputPath dist && ng run app:server:staging",
    "pwa": "npm run build:prod && http-server -p 8080 -c-1 dist\\prod",
    "prerender": "ng run app:prerender"
  },
